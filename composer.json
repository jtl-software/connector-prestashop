{
  "name": "jtl/connector-prestashop",
  "type": "app",
  "description": "PrestaShop Connector",
  "keywords": [
    "jtl",
    "Connector"
  ],
  "homepage": "http://www.jtl-software.de",
  "license": "LGPL-3.0-or-later",
  "authors": [
    {
      "name": "JTL Software GmbH",
      "email": "info@jtl-software.de",
      "homepage": "http://www.jtl-software.de",
      "role": "Founder"
    }
  ],
  "minimum-stability": "dev",
  "prefer-stable": true,
  "config": {
    "platform": {
      "php": "8.2"
    },
    "vendor-dir": "lib"
  },
  "require": {
    "php": ">=8.1",
    "jtl/connector": "^5.2.1",
    "symfony/yaml": "^3.4",
    "ext-json": "*",
    "phing/phing": "^2.16",
    "psr/cache": "^2",
    "monolog/monolog": "^2",
    "psr/log": "^2",
    "symfony/http-foundation": "^4.4"
  },
  "autoload": {
    "psr-4": {
      "jtl\\Connector\\Presta\\": "src/",
      "Tests\\": "tests/"
    },
    "psr-0": {
      "": "plugins/"
    }
  },
  "require-dev": {
    "friendsofphp/php-cs-fixer": "^2.16",
    "symfony/polyfill-php80": "<=1.18.1"
  }
}
